										<script>$("#bannedCount").text(parseInt($("#bannedCount").text()) + 1);</script>
										<div class="user" id="bannedUser-{{value._id}}">
											<div class="profilepic">
												<img src="{{value.picture}}" alt="">
											</div>
											<div class="username {{value.role}}">{{value.username}}</div>
											<div class="actions">
												<div class="rank">{{value.role}}</div>
												<div class="removeBan" onclick="banUser(this, { id: '{{value._id}}' }, 'User', false)"><i class="mdi mdi-24px mdi-close"></i></div>
											</div>
										</div>
										{{/if}}
										{{/each}}
