												{{/unlessEqual}}
												<div class="removeUser" onclick="banUser(this, { id: '{{value._id}}', username:'{{value.username}}', picture:'{{value.picture}}', role:'{{value.role}}' }, 'User', true)" title="Ban User"><i class="mdi mdi-24px mdi-close"></i></div>
												<div class="muteUser" onclick="muteUser(this, { id: '{{value._id}}', username:'{{value.username}}', picture:'{{value.picture}}', role:'{{value.role}}' }, true)"><i class="mdi mdi-24px mdi-volume-off"></i></div>
