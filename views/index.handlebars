							<button id="signin" class="sign-in" onclick="openTab(event, 'login')"><i class="mdi mdi-24px mdi-login-variant"></i> Sign in to play!</button>
							<button id="signup" class="sign-up" onclick="openTab(event, 'register')"><i class="mdi mdi-24px mdi-account-plus"></i> or Sign up</button>
							{{/unless}}
							{{#if isAuthenticated}}
							<button id="upvote" class="votelinks upvote" onclick="voteButton(event, 'upvote')"><i class="mdi mdi-24px mdi-thumb-up-outline"></i> 2</button>
							<button id="grab" class="votelinks grab" onclick="voteButton(event, 'grab')"><i class="mdi mdi-24px mdi-star-outline"></i> 5</button>
