												{{#unlessIncludes ../user.role "Moderator" value.role "VIP,Normal"}}{{#unlessIncludes ../user.role "Manager" value.role "Moderator,VIP,Normal"}}
												<div class="editUser" onclick="editStaffRole(this)"><i class="mdi mdi-24px mdi-pencil"></i></div>
												{{#ifEqualOr value.role "Normal" "VIP"}}
												{{#unlessEqual ../user.role "Moderator"}}
