													{{/each}}{{/ifEqual}}
													{{#ifEqual ../user.role "Manager"}}{{#each ../userRolesOfManager}}
													<option value="{{this}}" {{#ifEqual this value.role}}selected{{/ifEqual}}>{{this}}</option>
													{{/each}}{{/ifEqual}}
													{{#ifEqual ../user.role "Moderator"}}{{#each ../userRolesOfModerator}}
